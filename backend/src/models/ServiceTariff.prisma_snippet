
model ServiceTariff {
  id        Int      @id @default(autoincrement())
  name      String
  category  String   // Medical, Non-Medical, Admin
  price     Decimal  @db.Decimal(10, 2)
  unit      String?  // kali, hari, jam, pcs
  code      String?  @unique
  
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
